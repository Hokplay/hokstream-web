    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-fast-marquee": "^1.6.5",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.1.5",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1"
