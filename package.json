    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-valtio": "^0.6.1",
    "globals": "^15.14.0",
    "googleapis": "^144.0.0",
    "jest-extended": "^4.0.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^25.0.1",
    "nock": "^14.0.1",
    "playwright-core": "^1.51.0",
    "prettier": "^2.7.1",
    "rollup-plugin-react-scoped-css": "^1.1.0",
    "sass": "^1.54.5",
    "typescript": "^5.7.2",
    "unocss": "^0.65.3",
